<fieldset>
    <legend class="text-xl font-semibold leading-6 text-gray-900">
        <%= expi.header %>
    </legend>
    <div class="pt-4 grid max-w-2xl grid-cols-1 gap-x-6 gap-y-8 sm:grid-cols-6">

        <!-- Input Group -->
        <% expi.inputGroup.forEach((input) => { %> 
            <%- include('./input-profile', input) %>
        <% }); %>
        <!-- From/To -->
        <%- include('../atoms/date-input') %>

        <!-- Job Description -->
        <%- include('../atoms/textarea-profile') %>


    </div>
</fieldset>