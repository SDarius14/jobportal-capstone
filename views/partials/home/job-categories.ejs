<section class="bg-green-50">
  <div class=" py-32 sm:py-32">
    <div class="mx-auto max-w-screen-2xl px-6 lg:px-14">
      <div class="mx-auto max-w-2xl lg:mx-0">
        <h2 class="text-3xl font-bold tracking-tight text-gray-900 sm:text-5xl">
          <%= title %>
        </h2>
      </div>
    </div>
  </div>

  <!-- Cards -->
  <div class="flex justify-center">
    <ul
      role="list"
      class="grid grid-cols-1 gap-6 sm:grid-cols-2 md:grid-cols-2 lg:grid-cols-4 px-20">

      <!-- Cards Foreach -->
      <% cards.forEach((card) => { %> 
      <%- include('../../components/molecules/jobcategory-card', card) %> 
      <% }); %>

      <!-- Last Card Foreach -->
      <% cardsTwo.forEach((card) => { %> 
      <%- include('../../components/molecules/jobcategory-card-two', card) %> 
      <% }); %>
      
    <ul>
  <div>
</section>