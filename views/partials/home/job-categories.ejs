<section class="bg-[#f0f6f4]">
  <div class="mx-auto max-w-7xl px-4 py-24 sm:px-6 lg:px-8">
    <div class="rounded-2xl  px-6 py-16 sm:p-16">
      <div class="mx-auto max-w-2xl lg:mx-0 mb-8">
        <h2 class="text-4xl font-bold tracking-tight text-gray-900 sm:text-5xl">
          <%= title %>
        </h2>
      </div>
      <!-- Cards -->
      <div id="scale-up" class="flex justify-center">
        <ul
          role="list"
          class="grid grid-cols-1 gap-6 sm:grid-cols-2 md:grid-cols-3 lg:grid-cols-4"
        >
          <!-- Cards Foreach -->
          <% cards.forEach((card) => { %> <%-
          include('../../components/molecules/jobcategory-card', card) %> <% });
          %>

          <!-- Last Card Foreach -->
          <% cardsTwo.forEach((card) => { %> <%-
          include('../../components/molecules/jobcategory-card-two', card)%> <%
          }); %>
        </ul>
      </div>
    </div>
  </div>
</section>
