              <span class="hidden lg:flex lg:items-center">
                <span
                  class="ml-4 text-sm font-semibold leading-6 text-gray-900"
                  aria-hidden="true"
                  >Dani Martinez</span
                >
                <svg
                  class="ml-2 h-5 w-5 text-gray-400 transform transition-transform"
                  viewBox="0 0 20 20"
                  fill="currentColor"
                  aria-hidden="true"
                  :class="{'rotate-180': open}"
                >
                  <path
                    fill-rule="evenodd"
                    d="M5.23 7.21a.75.75 0 011.06.02L10 11.168l3.71-3.938a.75.75 0 111.08 1.04l-4.25 4.5a.75.75 0 01-1.08 0l-4.25-4.5a.75.75 0 01.02-1.06z"
                    clip-rule="evenodd"
                  />
                </svg>
              </span>
            </button>

            <!--
                    Dropdown menu, show/hide based on menu state.

                    Entering: "transition ease-out duration-100"
                      From: "transform opacity-0 scale-95"
                      To: "transform opacity-100 scale-100"
                    Leaving: "transition ease-in duration-75"
                      From: "transform opacity-100 scale-100"
                      To: "transform opacity-0 scale-95"
                  -->
            <div
              x-show="open"
              x-cloak
              @click.away="open = false"
              class="absolute right-0 z-10 mt-2.5 w-32 origin-top-right rounded-md bg-white py-2 shadow-lg ring-1 ring-gray-900/5 focus:outline-none"
              role="menu"
              aria-orientation="vertical"
              aria-labelledby="user-menu-button"
              tabindex="-1"
              x-transition:enter="transition ease-out duration-300 transform opacity-0"
              x-transition:enter-start="translate-y-[-10%] opacity-0"
              x-transition:enter-end="translate-y-0 opacity-100"
              x-transition:leave="transition ease-in duration-300 transform opacity-100"
              x-transition:leave-start="translate-y-0 opacity-100"
              x-transition:leave-end="translate-y-[-10%] opacity-0"
            >
